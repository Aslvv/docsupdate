{
  "name": "acoustic-docs",
  "version": "1.0.0",
  "license:": "MIT",
  "scripts": {
    "start": "docusaurus start",
    "build": "npm run format-docs && npm run format && docusaurus build",
    "dev": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "format-docs": "prettier --config .prettierrc --write \"**/*.{md,mdx}\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.9",
    "@docusaurus/plugin-sitemap": "^2.0.0-beta.9",
    "@docusaurus/preset-classic": "^2.0.0-beta.9",
    "classnames": "^2.3.1",
    "docusaurus-lunr-search": "^2.1.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/plugin-debug": "^2.0.0-beta.8",
    "prettier": "^2.4.1"
  }
