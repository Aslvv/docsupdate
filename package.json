{
  "name": "acoustic-docs",
  "version": "0.0.5",
  "scripts": {
    "start": "docusaurus start",
    "build": "npm run prettier-docs && npm run prettier && docusaurus build",
    "dev": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "prettier-docs": "prettier --config .prettierrc --write \"**/*.{md,mdx}\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.4",
    "@docusaurus/preset-classic": "^2.0.0-beta.4",
    "classnames": "^2.3.1",
    "docusaurus": "^2.0.0-alpha.378053ac5",
    "docusaurus-lunr-search": "^2.1.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.3.2"
  }
